void ss()
{
	vector<int> s{1,2,3,4,5,6};
	int i=0, sum=0;
	do{
		cout<<s[i]*s[i] << " ";
		sum=sum + s[i]*s[i];
		i++;
	} while(i<s.size());
	cout<< sum;
}

